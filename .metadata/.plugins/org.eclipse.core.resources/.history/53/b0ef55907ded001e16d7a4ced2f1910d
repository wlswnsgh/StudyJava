package loop;

public class Quiz16 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 3; i <= 9; i++) {
//			System.out.println(i+"ë‹¨");
			for(int j = 1; j <= 9; j++ ) {
				if(i % 2 == 0) break;{
					System.out.println(i+"X"+j+"="+i*j);
					
				}
			}
			System.out.println();
		}
	}

}
