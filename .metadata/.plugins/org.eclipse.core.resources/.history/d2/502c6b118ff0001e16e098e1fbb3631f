package Quiz;

public class Calculator 
{
	public static void CalEl(int num1, char ch ,int num2 ) {
		if(num1 + num2 == '+') {
			System.out.println(num1 + num2);
		} else if (num1 - num2 == '-') {
			System.out.println(num1 - num2);
		} else if (num1 * num2 == '*') {
			System.out.println(num1 * num2);
		} else {
			System.out.println(num1 / num2);
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(CalEl(10,5));
//		CalEl(10,+,5);
	}

}
