class Person {
	
	String name;
	
	int height;
	
	int weight;
	
	public Person() {	
	}

	public Person(String nm) {
		name = nm;
	}

	public Person(String nm, int he, int we) {
		name = nm;
		height = he;
		weight = we;
	}
	
}

