package loop;

public class Quiz17 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i = 1; i <= 3; i++) {
			for(int j = 0; j <= 4; j++) {
			System.out.print("*");			
			}
			System.out.println();
		}
		
		for(int j = 1; j <= 3; j++) {
			System.out.println('*');
		}
	}
}