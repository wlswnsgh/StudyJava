package Quiz;

public class instunt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		book book1 = new book();
		book1.tittle = "스프링부트";
		book1.Price = 30000;
		book1.ApublishingHouse = "한빛미디어";
		book1.NumberOfPages = 200;
		
		book book2 = new book("자바프로그래밍", 10000, "금빛미디어", 200);
	}

}

class book {
	String tittle;
	int Price;
	String ApublishingHouse;
	int NumberOfPages;
	
	public book() {
		
	}
	
	public book(String tittle, int price, String apublishingHouse, int numberOfPages) {
		tittle = tittle;
		Price = price;
		ApublishingHouse = apublishingHouse;
		NumberOfPages = numberOfPages;
	}
	
}