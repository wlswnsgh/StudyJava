package Quiz;

public class Comparison {
	
	public static int comparison(int n1, int n2) {
		if(n1 > n2) {
//			System.out.println(n1);
			return n1;
		} else {
//			System.out.println(n2);
			return n2;
		}
			
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		comparison(3, 4);
	}

}
