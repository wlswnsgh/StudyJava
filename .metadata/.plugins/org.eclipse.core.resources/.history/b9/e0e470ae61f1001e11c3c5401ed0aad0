package reference;

public class Quiz2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Department department = new Department();
		department.dpm = "jin";
		department.asr = "jun";
		department.alr = "ho";
		
		
	}

}

class  Department{
	String dpm;
	String asr;
	String alr;
	
	public Department() {
		
	}
	
	public Department(String dpm, String asr, String alr) {
			dpm = dpm;
			asr = asr;
			alr = alr;
	}
}	