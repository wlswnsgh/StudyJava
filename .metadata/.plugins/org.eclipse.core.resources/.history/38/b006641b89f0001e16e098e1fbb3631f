package Quiz;

public class division {
	
	public static int division(int n1, int n2) {
		
		int result = n1 / n2;
		
//		if(n2 == 0) {
//			return 0;
//		} 
		
		return result;

	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(division(10,2));
//		System.out.println(division(10,0));
	}

}
