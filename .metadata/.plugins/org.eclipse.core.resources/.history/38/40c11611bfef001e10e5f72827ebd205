
public class EX11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int j = 1; j <= 5; j++) {
			for(int k = 1; k <= j; k++) {
				System.out.print('*');
			}
			
			for(int l = 1; l >= j; l--) {
				System.out.println(' ');
			}
			
				System.out.println();
		}
		
	}
}
